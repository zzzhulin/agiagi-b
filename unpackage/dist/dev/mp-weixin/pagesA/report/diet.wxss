.report-diet-page {
  padding: 34rpx 32rpx 96rpx 32rpx;
}
.report-diet-page .gl-container {
  padding: 34rpx 32rpx;
  border-radius: 8rpx;
  background-color: #f9f9f9;
}
.report-diet-page .chart-container {
  width: 100%;
  height: 272rpx;
  box-shadow: 0 0 0 2rpx #e0e6f1 inset;
  overflow: hidden;
}
.report-diet-page .table-row {
  position: relative;
  height: auto;
}
.report-diet-page .table-row::after {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 2rpx;
  background-color: #e0e6f1;
}
.report-diet-page .table-col {
  position: relative;
  padding: 16rpx;
}
.report-diet-page .table-col::after {
  content: "";
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  width: 2rpx;
  background-color: #e0e6f1;
}
.report-diet-page .table-col:first-child {
  width: 166rpx;
  border-left: 2rpx solid #e0e6f1;
}
.report-diet-page .table-col:nth-child(2), .report-diet-page .table-col:nth-child(3) {
  width: 128rpx;
}
.report-diet-page .table-col:nth-child(4) {
  width: 264rpx;
}
.report-diet-page .nutrition-table .table-row:first-child:before {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  height: 2rpx;
  background-color: #e0e6f1;
}
