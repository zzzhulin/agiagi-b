.report-height-page {
  padding: 34rpx 32rpx 96rpx 32rpx;
}
.report-height-page .body-type {
  line-height: 1;
}
.report-height-page .body-type .text {
  display: inline;
}
.report-height-page .table-row {
  position: relative;
  height: auto;
}
.report-height-page .table-row::after {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 2rpx;
  background-color: #e0e6f1;
}
.report-height-page .table-col {
  position: relative;
  padding: 16rpx;
}
.report-height-page .table-col::after {
  content: "";
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  width: 2rpx;
  background-color: #e0e6f1;
}
.report-height-page .table-col:first-child {
  width: 176rpx;
  border-left: 2rpx solid #e0e6f1;
}
.report-height-page .table-col:nth-child(2) {
  width: 232rpx;
}
.report-height-page .table-col:nth-child(3) {
  width: 278rpx;
}
.report-height-page .table .table-row:first-child:before {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  height: 2rpx;
  background-color: #e0e6f1;
}
.report-height-page .height-analysis {
  line-height: 1;
}
.report-height-page .height-analysis .text {
  display: inline;
}
.report-height-page .medical-content {
  position: relative;
  padding-bottom: 48rpx;
}
.report-height-page .medical-content::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 2rpx;
  background-color: #e0e6f1;
}
